{
	"assistant": {
		"default_model": {
			"provider": "copilot_chat",
			"model": "gpt-4o"
		},
		"version": "2"
	},
	"vim_mode": true,
	"ui_font_size": 14,
	"buffer_font_size": 12,
	"buffer_font_family": "Hack Nerd Font",
	"theme": {
		"mode": "system",
		"light": "Solarized Light",
		"dark": "Yesterday Low"
	},
	"cursor_blink": false,
	"tab_size": 2,
	"lsp": {
		"biome": {
			"settings": {
				"require_config_file": false
			}
		}
	},
	"formatter": {
		"language_server": {
			"name": "biome"
		}
	}
}
